<h1>Users</h1>

<table class="table">
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link "View", to: user_path(@conn, :show, user) %> |
          <%= link "Edit", to: user_path(@conn, :edit, user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
